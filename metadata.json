{
  "name": "bodgit-microserver",
  "version": "1.0.0",
  "author": "bodgit",
  "summary": "Puppet Module for managing HP ProLiant MicroServers",
  "license": "Apache-2.0",
  "source": "https://github.com/bodgit/puppet-microserver",
  "project_page": "https://github.com/bodgit/puppet-microserver",
  "issues_url": "https://github.com/bodgit/puppet-microserver/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
