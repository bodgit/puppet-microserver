  "requirements": [
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "tags": [
    "hp",
    "proliant",
    "microserver",
    "ilo",
    "ipmi",
    "rng"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.0.0 <5.0.0"
    },
    {
      "name": "bodgit/rngd",
      "version_requirement": ">=1.0.0 <2.0.0"
    },
    {
      "name": "bodgit/watchdog",
      "version_requirement": ">=1.0.0 <2.0.0"
    }
  ]
